{
  "cleanUrls": true,
  "rewrites": [
      { "source": "/menu", "destination": "/menu.html" }
  ],
  "headers": [
      {
          "source": "/(.*).(css|js|png|jpg|jpeg|gif|ico|svg|webp|woff|woff2|ttf|otf|eot|ttc|map)",
          "headers": [{ "key": "Cache-Control", "value": "max-age=31536000" }]
      }
  ]
}
